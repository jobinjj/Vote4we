<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="sample.com.jobin.msi.vote4we.FinalActivity"
    android:background="#343746">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/img_adduser1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="40dp"
        android:text="" />

    <ImageView
        android:id="@+id/img_adduser1"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="98dp"
        android:scaleType="centerCrop"
        android:src="@drawable/ic_add_user"
        app:layout_constraintBottom_toTopOf="@+id/devider"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
         />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignTop="@+id/img_adduser1"
        android:layout_marginLeft="26dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="152dp"
        android:layout_toEndOf="@+id/textView4"
        android:layout_toRightOf="@+id/textView4"
        android:src="@drawable/ic_winner_gold_ribbon" />


</RelativeLayout>
